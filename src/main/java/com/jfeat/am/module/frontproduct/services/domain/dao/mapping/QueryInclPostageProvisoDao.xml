<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.frontproduct.services.domain.dao.QueryInclPostageProvisoDao">
                                                                                                                                                                                                                                                                                                                                                <sql id="Base_Column_List">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        t_incl_postage_proviso.id, t_incl_postage_proviso.fare_id AS fareId, t_incl_postage_proviso.region, t_incl_postage_proviso.piece_no AS pieceNo, t_incl_postage_proviso.weight_no AS weightNo, t_incl_postage_proviso.bulk_no AS bulkNo, t_incl_postage_proviso.amount, t_incl_postage_proviso.carry_way AS carryWay, t_incl_postage_proviso.type
    </sql>



    <select id="findInclPostageProvisoPage" resultType="InclPostageProvisoRecord" parameterType="InclPostageProvisoRecord">
            SELECT
        <include refid="Base_Column_List" />
        FROM t_incl_postage_proviso
        WHERE 1=1
                                <if test="record.id != null and record.id>0 ">
                AND t_incl_postage_proviso.id LIKE CONCAT('%',#{record.id},'%')
            </if>
                                                                                            <if test="record.fareId != null and record.fareId>0 ">
                AND t_incl_postage_proviso.fare_id LIKE CONCAT('%',#{record.fareId},'%')
            </if>
                                <if test="record.region != null and record.region!= ''">
                AND t_incl_postage_proviso.region LIKE CONCAT('%',#{record.region},'%')
            </if>
                                                                                            <if test="record.pieceNo != null and record.pieceNo>0 ">
                AND t_incl_postage_proviso.piece_no LIKE CONCAT('%',#{record.pieceNo},'%')
            </if>
                                                                                            <if test="record.weightNo != null and record.weightNo>0 ">
                AND t_incl_postage_proviso.weight_no LIKE CONCAT('%',#{record.weightNo},'%')
            </if>
                                                                                            <if test="record.bulkNo != null and record.bulkNo>0 ">
                AND t_incl_postage_proviso.bulk_no LIKE CONCAT('%',#{record.bulkNo},'%')
            </if>
                                <if test="record.amount != null and record.amount>0 ">
                AND t_incl_postage_proviso.amount LIKE CONCAT('%',#{record.amount},'%')
            </if>
                                                                                            <if test="record.carryWay != null and record.carryWay>0 ">
                AND t_incl_postage_proviso.carry_way LIKE CONCAT('%',#{record.carryWay},'%')
            </if>
                                <if test="record.type != null and record.type>0 ">
                AND t_incl_postage_proviso.type LIKE CONCAT('%',#{record.type},'%')
            </if>
                <if test="startTime != null">
            <![CDATA[AND t_incl_postage_proviso.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND t_incl_postage_proviso.end_time <= DATE(#{endTime)]]>
        </if>
            <!--
        <if test="search != null and search != ''">
            OR t_incl_postage_proviso.name LIKE CONCAT('%',#{search},'%')
        </if>
        -->
                                    </select>
</mapper>