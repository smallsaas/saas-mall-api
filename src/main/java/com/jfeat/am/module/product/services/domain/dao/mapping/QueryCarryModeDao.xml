<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.product.services.domain.dao.QueryCarryModeDao">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                <sql id="Base_Column_List">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        t_carry_mode.id, t_carry_mode.fare_id AS fareId, t_carry_mode.region, t_carry_mode.first_piece AS firstPiece, t_carry_mode.first_weight AS firstWeight, t_carry_mode.first_bulk AS firstBulk, t_carry_mode.first_amount AS firstAmount, t_carry_mode.second_piece AS secondPiece, t_carry_mode.second_weight AS secondWeight, t_carry_mode.second_bulk AS secondBulk, t_carry_mode.second_amount AS secondAmount, t_carry_mode.carry_way AS carryWay, t_carry_mode.is_default AS isDefault
    </sql>



    <select id="findCarryModePage" resultType="CarryModeRecord" parameterType="CarryModeRecord">
            SELECT
        <include refid="Base_Column_List" />
        FROM t_carry_mode
        WHERE 1=1
                                <if test="record.id != null and record.id>0 ">
                AND t_carry_mode.id LIKE CONCAT('%',#{record.id},'%')
            </if>
                                                                                            <if test="record.fareId != null and record.fareId>0 ">
                AND t_carry_mode.fare_id LIKE CONCAT('%',#{record.fareId},'%')
            </if>
                                <if test="record.region != null and record.region!= ''">
                AND t_carry_mode.region LIKE CONCAT('%',#{record.region},'%')
            </if>
                                                                                            <if test="record.firstPiece != null and record.firstPiece>0 ">
                AND t_carry_mode.first_piece LIKE CONCAT('%',#{record.firstPiece},'%')
            </if>
                                                                                            <if test="record.firstWeight != null and record.firstWeight>0 ">
                AND t_carry_mode.first_weight LIKE CONCAT('%',#{record.firstWeight},'%')
            </if>
                                                                                            <if test="record.firstBulk != null and record.firstBulk>0 ">
                AND t_carry_mode.first_bulk LIKE CONCAT('%',#{record.firstBulk},'%')
            </if>
                                                                                            <if test="record.firstAmount != null and record.firstAmount>0 ">
                AND t_carry_mode.first_amount LIKE CONCAT('%',#{record.firstAmount},'%')
            </if>
                                                                                            <if test="record.secondPiece != null and record.secondPiece>0 ">
                AND t_carry_mode.second_piece LIKE CONCAT('%',#{record.secondPiece},'%')
            </if>
                                                                                            <if test="record.secondWeight != null and record.secondWeight>0 ">
                AND t_carry_mode.second_weight LIKE CONCAT('%',#{record.secondWeight},'%')
            </if>
                                                                                            <if test="record.secondBulk != null and record.secondBulk>0 ">
                AND t_carry_mode.second_bulk LIKE CONCAT('%',#{record.secondBulk},'%')
            </if>
                                                                                            <if test="record.secondAmount != null and record.secondAmount>0 ">
                AND t_carry_mode.second_amount LIKE CONCAT('%',#{record.secondAmount},'%')
            </if>
                                                                                            <if test="record.carryWay != null and record.carryWay>0 ">
                AND t_carry_mode.carry_way LIKE CONCAT('%',#{record.carryWay},'%')
            </if>
                                                                                            <if test="record.isDefault != null and record.isDefault>0 ">
                AND t_carry_mode.is_default LIKE CONCAT('%',#{record.isDefault},'%')
            </if>
                <if test="startTime != null">
            <![CDATA[AND t_carry_mode.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND t_carry_mode.end_time <= DATE(#{endTime)]]>
        </if>
            <!--
        <if test="search != null and search != ''">
            OR t_carry_mode.name LIKE CONCAT('%',#{search},'%')
        </if>
        -->
                                    </select>
</mapper>